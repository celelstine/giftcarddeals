{
  "name": "exchangezone9ja",
  "version": "1.0.0",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "description": "A platform to sell itunecards",
  "author": "celelstine <okorocelestine@gmail.com>",
  "private": true,
  "scripts": {
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "nodemon node lib/server.js",
    "pm2:start": "node lib/server.js",
    "unit": "jest --config test/unit/jest.conf.js --cove444rage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build_client_only": "node build/build.js",
    "build": "npm run prepare_dir && npm run build:client",
    "prepare_dir": "rimraf lib  && mkdir lib  && mkdir lib/api && mkdir lib/dist && mkdir lib/giftcards",
    "build:client": "npm run build_client_only && npm run build:server",
    "build:server": "babel api  -d lib/api --copy-files --ignore tests/ && npm run build-app",
    "build-app": "mkdir lib/dist/productImages  && babel server.js  -d lib --copy-files && npm run db:migrate && node lib/server.js",
    "db:migrate": "sequelize db:migrate"
  },
